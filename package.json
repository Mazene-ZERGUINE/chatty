    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^9.1.7"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:frontend": "cd frontend && npm run lint:check",
